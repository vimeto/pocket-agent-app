{
  "expo": {
    "name": "PocketAgent",
    "slug": "pocket-agent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pocketagent.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.pocketagent.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "./expo-plugins/llama-rn-plugin.js",
      "expo-dev-client",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          },
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "minSdkVersion": 21,
            "ndkVersion": "24.0.8215888"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a3581315-f8c1-497e-b53f-225a7ada80ee"
      }
    },
    "owner": "vimetoivonen"
  }
}
